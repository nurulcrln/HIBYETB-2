<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../../master/header'); %>
</head>
<body class="container">

<header>
  <%- include('../../master/navbar'); %>
</header>


<main>

    <form action="/dokter" method="post" enctype="multipart/form-data">
        
        <div class="form-group">
            <label for="nama-dokter" class="col-form-label">Nama :</label>
            <input type="text" class="form-control" name="nama" id="nama-dokter" value="<%= nama %>"> 
        </div>

        <div class="form-group">
            <label for="email-dokter" class="col-form-label">Email :</label>
            <input type="email" class="form-control" name="email" id="email-dokter" value="<%= email %>">
        </div>

        <div class="form-group">
            <label for="phone-dokter" class="col-form-label">Phone :</label>
            <input type="number" class="form-control" name="phone" id="phone-dokter" value="<%= phone %>">
        </div>

        <div class="form-group">
            <label for="spesialis-dokter" class="col-form-label">Spesialis :</label>
            <input type="text" class="form-control" name="spesialis" id="spesialis-dokter" value="<%= spesialis %>">
        </div>

        <div class="form-group">
            <label for="praktik-dokter" class="col-form-label">Tempat Praktik :</label>
            <input type="text" class="form-control" name="tempat_praktik" id="praktik-dokter" value="<%= tempat_praktik %>">
        </div>

        <!-- <div class="form-group">
            <label for="img-dokter" class="col-form-label">Upload Image</label>
            <input type="file" class="form-control" name="image"  id="img-dokter" value="<%= url_foto %>">
        </div>     -->
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>

</main>

<footer>
  <%- include('../../master/footer'); %>
</footer>

</body>
</html>