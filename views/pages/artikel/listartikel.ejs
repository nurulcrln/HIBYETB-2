<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../../master/header'); %>
</head>
<header>
    <%- include('../../master/navbar'); %>
</header>
<body>
    <main class="list-artikel">
        <form class="form container-fluid src-artikel ">
            <input class="form-control mr-sm-2 boxSearch shadow-lg material-symbols-outlined" type="search" placeholder="Search" aria-label="Search">
        </form>
        <div id="grid" class="listSearch">
            <% if(data.length){ for(var i=0; i< data.length; i++) {%>
                <article>
                    <div>
                        <div><br>
                            <img src=" <%= data[i].url_foto%>"  id="img-listartikel" alt="Hak Cipta HIByeTB">
                            <div>
                                <h5 class="title-artikel p-5">
                                    <%= data[i].judul%>
                                </h5>
                                <a href="/listitem/<%= data[i].id%>" class="btn btn-primary"
                                    id="detail-artikel">Detail</a><br><br>
                            </div>
                        </div>
                    </div>
                </article>
                <% } }else{ %>
                    <div class="alert alert-danger" role="alert">
                        Data Tidak Ditemukan !
                    </div>
                    <% } %>

        </div>
    </main>
